        "@babel/preset-react": "^7.12.13",
        "@babel/preset-typescript": "^7.12.13",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-redux": "^7.1.7",
        "@types/react-router": "^5.1.11",
