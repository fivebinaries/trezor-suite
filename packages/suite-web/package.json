        "path-browserify": "^1.0.1",
        "react-refresh": "^0.9.0",
        "stream-browserify": "^3.0.0",
        "ts-loader": "^8.0.14",
        "webpack": "^5.21.2",
        "webpack-bundle-analyzer": "^4.4.0",
